<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WebBrowser"
        xmlns:Wpf="clr-namespace:Microsoft.Web.WebView2.Wpf;assembly=Microsoft.Web.WebView2.Wpf" x:Class="WebBrowser.MainWindow"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <!--アドレスバー-->
            <RowDefinition Height="Auto"/>
            <RowDefinition/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <DockPanel Grid.Row="0" Margin="5,5,5,5">
            <Button Content="戻る" Width="60" Margin="0,0,5,0" Click="Button_Click"/>
            <Button Content="進む" Width="60" Margin="0,0,5,0" Click="Button_Click_1"/>
            <TextBox x:Name="AddressBar" Margin="0,0,5,0" Width="550"/>
            <Button Content="移動" Width="60" Margin="0,0,5,0" Click="GoButton_Click"/>
        </DockPanel>
        <ProgressBar x:Name="LoadingBar" HorizontalAlignment="Left" Height="10" Margin="0,0,0,0" Grid.Row="2" VerticalAlignment="Top" Minimum="0" Maximum="100" Visibility="Collapsed" IsIndeterminate="True"/>
        <Wpf:WebView2 x:Name="WebView" Margin="5,5,5,5" Grid.Row="2"/>

    </Grid>
</Window>
